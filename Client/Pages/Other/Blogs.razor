@page "/blog"
@page "/blogs"

<MudPaper Elevation="1" Class="pa-4 my-2">
    <MudText Typo="Typo.h5">Latest Blog Posts</MudText>
    @foreach (var post in blogPosts)
    {
        <MudCard Class="my-2">
            <MudCardContent>
                <MudText Typo="Typo.h6">@post.Title</MudText>
                <MudText>@post.Summary</MudText>
            </MudCardContent>
            <MudCardActions>
                <MudButton Variant="Variant.Text" Color="Color.Primary">Read More</MudButton>
            </MudCardActions>
        </MudCard>
    }
</MudPaper>

@code {
    private List<BlogPost> blogPosts = new List<BlogPost>
    {
        new BlogPost { Title = "Exploring Blazor", Summary = "A deep dive into Blazor's components..." },
        new BlogPost { Title = "MudBlazor in Action", Summary = "How to build beautiful UI with MudBlazor..." },
        // Add more blog posts as needed
    };

    public class BlogPost
    {
        public string Title { get; set; }
        public string Summary { get; set; }
    }
}



